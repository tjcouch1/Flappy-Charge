    draw_text(room_width / 2, room_height / 2 - strHt * 2, "Flappy Charge");
    draw_text(room_width / 2, room_height / 2 - strHt * 1, "Up, Space, MouseL: Jump!");
    draw_text(room_width / 2, room_height / 2, "Right, C, MouseR: Charge!");
    draw_text(room_width / 2, room_height / 2 + strHt * 1, "High Score: " + string(highscr));
    draw_text(room_width / 2, room_height / 2 + strHt * 2, "Press Jump to Start!");
}
