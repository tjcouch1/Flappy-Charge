            <string>if (keyboard_check_pressed(vk_up) || keyboard_check_pressed(vk_space) || mouse_check_button_pressed(mb_left))
{
    ymov = -jumpspd;
    
    if (!start || dead)
    {
        start = true;
        firststart = true;
        x = xstart;
        y = ystart;
        charge = 0;
        dead = false;
        with (obj_tube)
            if (object_index != obj_floor)
                instance_destroy();
    }
}

if (firststart)
{
    if (start &amp;&amp; !dead)
    {
        if (charge &lt; -nocharge &amp;&amp; keyboard_check_pressed(vk_right) || keyboard_check_pressed(ord("C")) || mouse_check_button_pressed(mb_right))
        {
            //start charge
            charge = chargecap;
        }
    }
    
    if (charge == 1)
    {
        if (!breakblocks)
            player_die();
        breakblocks = false;
    }
    
    if (charge &lt;= 0)
    {
        xmov = spd;
        ymov += grav;
    }
    else
    {
        xmov = chargespd;
        ymov = 0;
    }
    
    charge--;
    difficulty += difficultyinc;
}

y += ymov;

//move stuff
if (!dead)
{
    var gained_point = false;
    with (all)
        if (object_index != obj_player)
        {
            x -= other.xmov;
            
            //points
            var spr_w = sprite_get_width(sprite_index);
            if (!gained_point &amp;&amp; object_index != obj_floor &amp;&amp; other.x &gt; x + spr_w / 2 &amp;&amp; other.x - other.xmov &lt;= x + spr_w / 2)
            {
                other.scr++;
                gained_point = true;
            }
        }
}

image_angle = point_direction(x - xmov, yprev - (yprev - y) / 2, x, y);

if (start)
{
    //check collision for death or breaking blocks
    for (var i = 0; i &lt; instance_number(obj_tube); i++)
    {
        var block = instance_find(obj_tube, i);
        if (place_meeting(x, y, block))
            if (charge &gt; 0 &amp;&amp; block.br)
                with (block)
                {
                    instance_destroy();
                    other.breakblocks = true;
                }
            else player_die();
    }
    
    //create blocks
    if (difficulty mod spacing == 0)
    {
        var opensize = 5;
        var openspot = irandom(room_height / sprite_get_height(spr_tube) - 12) + 2;
        var cracked = random(1) &lt; .2;
        for (var i = 0; i &lt; room_height / sprite_get_height(spr_tube); i++)
        {
            if (i &lt; openspot || i &gt;= openspot + opensize)
            {
                instance_create(room_width, i * sprite_get_height(spr_tube), obj_tube);
            }
            else if (cracked)
            {
                instance_create(room_width, i * sprite_get_height(spr_tube), obj_tube_cracked);
            }
        }
    }
}

//check dead collision
if (dead)
{
    for (var i = 0; i &lt; instance_number(obj_tube); i++)
    {
    }
}

yprev = y;
